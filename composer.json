{
    "name": "imrealashu/multiauth-generator",
    "description": "Laravel 5.2 Package for Generating Multiple Authentication",
    "keywords": [
        "laravel",
        "Laravel multiauth",
        "multi-auth",
        "Laravel 5.2"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Ashish Singh",
            "email": "imrealashu@gmail.com"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "Imrealashu\\MultiAuthGenerator\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "imrealashu\\MultiAuthGenerator\\Test\\": "tests"
        }
    }
}
